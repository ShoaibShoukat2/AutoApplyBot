{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(static/.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(media/.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "core.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "build": {
    "env": {
      "PYTHONPATH": "core"
    }
  }
}

